{
  "extends": [
    "config:base"
    ":dependencyDashboardApproval"
  ],
  "schedule": [
    "every sunday"
  ]
}
{
  "packageRules": [
    {
      "matchUpdateTypes": ["major"],
      "matchManagers": ["npm"],
      "matchPackagePatterns": "^npm",
      "groupName": ["NPM Update"],
      "assignees": ["TheKingTermux"],
      "label": ["Bot Update", "dependencies", "Auto Updated"]
      "dependencyDashboardApproval": true
    }
  ]
}
